<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fbutton=""
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.cfg.team12.makeawish.VolunteerDashboardActivity"
    tools:showIn="@layout/app_bar_volunteer_dashboard">
    <android.support.v7.widget.RecyclerView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/recycler_view"
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:scrollbars="none"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp"></android.support.v7.widget.RecyclerView>
    <info.hoang8f.widget.FButton
        android:id="@+id/btn_data"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:text="Next"
        fbutton:buttonColor="@color/fbutton_color_emerald"
        fbutton:cornerRadius="5dp"
        fbutton:shadowColor="@color/fbutton_color_green_sea"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="5dp"
        tools:layout_editor_absoluteY="447dp"
        tools:layout_editor_absoluteX="280dp" />

</android.support.constraint.ConstraintLayout>
